<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ActivityData" table="activity_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activityId" type="integer" column="activity_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dataId" type="integer" column="data_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="defaultValue" type="string" column="default_value" length="18" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="required" type="integer" column="required" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="string" column="created" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastModified" type="string" column="last_modified" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
