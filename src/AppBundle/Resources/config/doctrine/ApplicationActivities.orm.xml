<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ApplicationActivities" table="application_activities">
    <indexes>
      <index name="application_id" columns="application_id"/>
      <index name="activity_id" columns="activity_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="applicationId" type="integer" column="application_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activityId" type="integer" column="activity_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="active" type="integer" column="active" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activeStart" type="string" column="active_start" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activeEnd" type="string" column="active_end" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="string" column="created" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastModified" type="string" column="last_modified" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
