    <field name="activityId" type="integer" column="activity_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="required" type="integer" column="required" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="string" column="created" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
