<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Brands" table="brands">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="string" column="code" length="1" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="binCode" type="integer" column="bin_code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isMulti" type="integer" column="is_multi" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="string" column="created" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastModified" type="string" column="last_modified" length="19" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
