<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ImportGiodoDefinition" table="import_giodo_definition">
    <unique-constraints>
      <unique-constraint name="code" columns="code"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="string" column="code" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="crmCode" type="string" column="crm_code" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="activeFrom" type="datetime" column="active_from" nullable="true"/>
    <field name="activeTo" type="datetime" column="active_to" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="modified" type="datetime" column="modified" nullable="true"/>
  </entity>
</doctrine-mapping>
